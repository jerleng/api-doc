<ul class="nav navbar-nav" id="mobile-nav-list">
<li><a title="Home" class="apidoc-link" data-ref="intro" rel="api">Home</a></li>
{{#if apis}}
<li class="dropdown">
	<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">APIs <span class="caret"></span></a>
	<ul class="dropdown-menu">
{{#eachInMap apis}}
		{{#if key}}
    	<li class="dropdown-header">{{key}}</li>		
		{{else}}
    	<li class="dropdown-header">General</li>		
		{{/if}}
		{{#each value}}
		<li class="dropdown">
			<li><a title="{{description}}" class="apidoc-link" data-ref="{{ref}}" rel="api">{{name}}</a></li>
			{{#each methods}}
			<li><a title="{{description}}" class="apidoc-link"  data-ref="{{ref}}" rel="method"><small>&nbsp;&nbsp;&nbsp;{{name}}</small></a></li>
			{{/each}}
		</li>
		{{/each}}
{{/eachInMap}}
	</ul>
</li>
{{/if}}
{{#if objects}}
<li class="dropdown">
	<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Objects <span class="caret"></span></a>
	<ul class="dropdown-menu">
{{#eachInMap objects}}
		{{#if key}}
    	<li class="dropdown-header">{{key}}</li>		
		{{else}}
    	<li class="dropdown-header">General</li>		
		{{/if}}
		{{#each value}}
		<li><a title="{{description}}" class="apidoc-link"  data-ref="{{ref}}" rel="object">{{name}}</a></li>
		{{/each}}
{{/eachInMap}}
	</ul>
</li>
{{/if}}
{{#if flows}}
<li class="dropdown">
	<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Flows <span class="caret"></span></a>
	<ul class="dropdown-menu">
{{#eachInMap flows}}
		{{#if key}}
    	<li class="dropdown-header">{{key}}</li>		
		{{else}}
    	<li class="dropdown-header">General</li>		
		{{/if}}
		{{#each value}}
		<li><a title="{{description}}" class="apidoc-link"  data-ref="{{ref}}" rel="object">{{name}}</a></li>
		{{/each}}
{{/eachInMap}}
	</ul>
</li>
{{/if}}
</ul>
