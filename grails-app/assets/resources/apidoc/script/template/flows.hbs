{{#if flows}}
{{#eachInMap flows}}
	<ul class="list-unstyled">
		{{#if key}}
		<li style="text-transform: uppercase;margin-top:5px;">{{key}}</li>
		{{else}}
		<li style="text-transform: uppercase;margin-top:5px;">General</li>
		{{/if}}
		{{#each value}}
			<li><a title="{{description}}" class="apidoc-link"  data-ref="{{ref}}" rel="flow">{{name}}</a></li>
		{{/each}}
	</ul>
{{/eachInMap}}
{{else}}
<p>No flows documented</p>
{{/if}}
